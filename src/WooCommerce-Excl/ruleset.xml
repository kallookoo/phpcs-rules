<?xml version="1.0"?>
<ruleset name="WooCommerce-Excl">
	<description>WooCommerce rules that exclude your own files.</description>

	<!-- WooCommerce plugins -->
	<exclude-pattern>*/plugins/(woocommerce|action-scheduler)/*</exclude-pattern>
	<!-- Inherit exclude patterns and configurations. -->
	<rule ref="WordPress-Excl"/>
	<!-- WooCommerce -->
	<rule ref="WooCommerce-Core"/>
</ruleset>
